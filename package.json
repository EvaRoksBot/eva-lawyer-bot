{
  "name": "eva-lawyer-bot-ultimate",
  "version": "5.0.0",
  "description": "Eva Lawyer Bot Ultimate - Advanced AI-powered legal assistant with document processing, analytics, and comprehensive integrations",
  "main": "api/telegram-ultimate-v5.js",
  "scripts": {
    "dev": "vercel dev",
    "build": "echo 'No build step required for serverless functions'",
    "start": "node dist/api/telegram.js",
    "deploy": "vercel --prod",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "telegram",
    "bot",
    "lawyer",
    "legal",
    "ai",
    "chatgpt",
    "openai",
    "vercel",
    "serverless",
    "typescript"
  ],
  "author": "EvaRoksBot Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EvaRoksBot/eva-lawyer-bot.git"
  },
  "bugs": {
    "url": "https://github.com/EvaRoksBot/eva-lawyer-bot/issues"
  },
  "homepage": "https://github.com/EvaRoksBot/eva-lawyer-bot#readme",
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "openai": "^4.67.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.6",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "vercel": "^39.2.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "api/**/*.ts",
      "!**/*.d.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  }
}
